    "egg-passport-local": "^1.2.1",
    "egg-scripts": "^2.11.0",
    "egg-sequelize": "^5.1.0",
    "egg-validate": "^2.0.2",
    "lodash": "^4.17.11",
    "mysql2": "^1.6.5",
    "node-xlsx": "^0.15.0",
    "svg-captcha": "^1.4.0",
    "validator": "^11.0.0"
